#include <bits/stdc++.h>
using namespace std;
int a[1000005];
int main() {
	int t,n;
	scanf("%d", &t);
	for(int i=1;i<=t;i++){
		scanf("%d", &n);
		for(int i=0;i<n;i++){
			cin>>a[i];
		}
		bool result=false;
		for(int i=0;i<n;i++){
		if(i){
				if(a[i]-a[i-1]==1){
			result=true;
			break;
			}
		}
		
	}
	if(result)
	printf("Case %d: Yes\n",i);
	else
	printf("Case %d: No\n",i);
   }
	return 0;
}
